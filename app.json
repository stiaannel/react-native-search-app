{
  "expo": {
    "name": "react-native-robofriends",
    "slug": "react-native-robofriends",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "React native app based on https://github.com/aneagoie/robofriends",
    "githubUrl": "https://github.com/stiaannel/robofriends-react-native"
  }
}
